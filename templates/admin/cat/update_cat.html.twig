{% extends 'base.html.twig' %}

{% block body_id 'update_cat' %}

{% block main %}
    {{ form_start(form) }}
        {{ form_row(form.catName) }}
    <button type="submit" class="btn btn-primary">
        <i class="fa fa-save" aria-hidden="true"></i><label>Update Category</label>
    </button>
    {{ form_end(form) }}
{% endblock %}

{% block sidebar %}
    <div class="section">
        <a href="{{ path('view_cats') }}">View All Categories</a>
    </div>

    {{ parent() }}
{% endblock %}