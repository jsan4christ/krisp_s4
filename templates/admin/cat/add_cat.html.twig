{% extends 'base.html.twig' %}

{% block body_id 'add_cat' %}

{% block main %}
    {{ form_start(form) }}
        {{ form_row(form.catName) }}
    <button type="submit" class="btn btn-primary">
        <i class="fa fa-save" aria-hidden="true"></i><label>Add Category</label>
    </button>
    {{ form_end(form) }}
{% endblock %}

{% block sidebar %}
        <div class="section">
            <a href="{{ path('view_cats') }}"><i class="fa fa-arrow-left"></i>View All Categories</a>
        </div>

    {{ parent() }}
{% endblock %}